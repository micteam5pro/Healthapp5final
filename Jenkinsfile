pipeline{
    agent any 
    stages{
        stage('1-clone'){
            steps{
                sh 'whoami'
            }
        }
        stage('2-Michael-statistic'){
            steps{
                sh 'lscpu'
            }
        }
        stage('3-Michael-location'){
            steps{
                sh 'pwd'
            }
        }
        stage('4-James-list'){
            steps{
                sh 'ls'
            }
        }
        stage('5-James-logs'){
            steps{
                sh 'echo "healthapp" '
            }
        }
        stage('6-Chioma-systemcheck'){
            steps{
                sh 'uptime'
            }
        }
        stage('7-Chioma-instruction'){
            steps{
                sh '#!/bin/bash'
            }
        }
        stage('8-Ekemini-systemcheck'){
            steps{
                sh 'sudo systemctl status jenkins'
            }
        }
        stage('9-Ekemini-network info'){
            steps{
                sh 'cat /etc/passwd'
            }
        }
        stage('10-Chris-exitstatus'){
            steps{
                sh 'echo $?'
            }
        }
        stage('11-Chris-sort'){
            steps{
                sh 'sort -r'
            }
        }
        stage('12-Caleb-processrunning'){
            steps{
                sh 'ps -ef'
            }
        }
        stage('13-Caleb-backendversion'){
            steps{
                sh 'uname -r'
            }
        }
        
    }
}