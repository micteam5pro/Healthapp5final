pipeline{
    agent any
    stages{
        stage('1-clone'){
            steps{
                sh 'pwd'
            }
        }
        stage('2-test'){
            steps{
                sh 'lsblk'
            }
        }
        stage('3-testing'){
            steps{
                sh 'll'
            }
        }
        stage('4-dir'){
            steps{
                sh 'mkdir'
            }
        }
        stage('5-list'){
            steps{
                sh 'ls'
            }
        }
        stage('6-word count'){
            steps{
                sh 'wc -l'
            }
        }stage('7-loginfo'){
            steps{
                sh 'whoami'
            }
        }
        stage('8-network'){
            steps{
                sh 'ip -a'
            }
        }
        stage('9-message'){
            steps{
                sh 'Welcome to Health app Group 5'
            }
        }
    }
}